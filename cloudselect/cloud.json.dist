{
  "log": {
    "formatters": {
      "f": {
        "format": "%(asctime)s %(name)-12s %(levelname)-8s %(message)s"
      }
    },
    "handlers": {
      "h": {
        "class": "logging.StreamHandler",
        "formatter": "f",
        "level": 10
      }
    },
    "root": {
      "handlers": [
        "h"
      ],
      "level": 40
    },
    "version": 1
  },
  "plugin": {
    "aws": "cloudselect.discovery.aws",
    "json_report": "cloudselect.report.json",
    "simple_filter": "cloudselect.filter.simple"
  }
}
